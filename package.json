  "scripts": {
    "start": "babel-watch server.js webpack -w",
    "start:dev": "babel-watch server.js",
    "webpack:dev": "webpack -w",
    "webpack:prod": "webpack -p",
    "test-server": "NODE_ENV=test mocha --compilers js:babel-core/register server/tests/**/**.spec.js",
    "test": "cross-env NODE_ENV=test npm run db:test:drop && npm run migrate:test:db && npm run db:test:seed && npm run coverage ",
    "migrate:db": "NODE_ENV=development sequelize db:migrate",
    "db:test:seed": "NODE_ENV=test sequelize db:seed:all",
    "db:test:drop": "NODE_ENV=test sequelize db:migrate:undo:all",
    "migrate:test:db": "NODE_ENV=test sequelize db:migrate",
    "e2e": "node_modules/.bin/nightwatch --config nightwatch.config.js",
    "coverage": "cross-env NODE_ENV=test node_modules/.bin/istanbul cover  ./node_modules/.bin/_mocha  server/tests/**/**.spec.js -- --require babel-register --timeout 90000"
  },
